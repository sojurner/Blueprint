    "styled-components": "^3.5.0-0",
    "styled-theme": "^0.3.3",
    "styled-tools": "^1.6.0"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react-typescript@3.0.1"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
